# 阿里云认证（必填）
ALIYUN_ACCESS_KEY_ID=your-access-key-id
ALIYUN_ACCESS_KEY_SECRET=your-access-key-secret

# Telegram 通知配置（必填）
TELEGRAM_ENABLED=true
TELEGRAM_BOT_TOKEN=your-bot-token
TELEGRAM_CHAT_ID=your-chat-id

# 检测间隔（秒），默认 60
CHECK_INTERVAL=60

# 重试配置
RETRY_COUNT=3          # 启动失败重试次数，默认 3
RETRY_INTERVAL=30      # 重试间隔（秒），默认 30

# 通知限流
NOTIFY_COOLDOWN=300    # 同一实例通知冷却时间（秒），默认 300

# 健康检查配置
HEALTH_CHECK_ENABLED=true    # 是否启用健康检查，默认 true
HEALTH_CHECK_TIMEOUT=300     # 健康检查超时时间（秒），默认 300
HEALTH_CHECK_INTERVAL=10     # 健康检查间隔（秒），默认 10

# 日志配置
LOG_LEVEL=info         # 日志级别：debug/info/warn/error，默认 info
LOG_FILE=